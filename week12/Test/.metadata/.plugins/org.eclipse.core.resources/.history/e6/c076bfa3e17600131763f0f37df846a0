
public class Node 
{
	
	private Node above;
	private Node below;
	
	public Node ()
	{
		
			
	}
	
	public void setAbove(Node newNode)
	{
		this.above = newNode;		
	}
	
	public void setBelow (Node newNode)
	{
		this.below = newNode;
		
	}
	
	public Node getBelow()
	{
		return this.below;
	}
	
	public Node getAbove()
	{
		return this.above;
	}
}

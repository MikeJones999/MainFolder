
public class Node 
{
	private String name;
	private Node above;
	private Node below;
	
	public Node (String name)
	{
		this.name = name;
			
	}
	
	public void setAbove(Node newNode)
	{
		this.above = newNode;		
	}
	
	public void setBelow (Node newNode)
	{
		this.below = newNode;
		
	}
	
	public String getBelow()
	{
		return this.below.getName();
	}
	
	public String getAbove()
	{
		return this.above.getName();
	}
	
	public String getName()
	{
		return this.name; 
		
	}
	
}



